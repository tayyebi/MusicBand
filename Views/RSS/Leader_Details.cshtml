@model Symphony.Models.Leader

@{
    ViewBag.Title = Model.Fullname;
}

<fullname>
    @Html.DisplayFor(model => model.Fullname)
</fullname>
<birth>
    @Html.DisplayFor(model => model.BirthYear)
</birth>
<text>
    @Html.Raw(Model.Text)
</text>
<thumbnail>
    @Symphony.Controllers.About.Root.ToString()Thumbs/Leader/@Model.Id
</thumbnail>
